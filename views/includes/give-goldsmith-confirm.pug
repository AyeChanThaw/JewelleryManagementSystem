div.modal.fade#dialogGiveGoldsmithConfirm(tabindex='-1', role='dialog', aria-labelledby='giveGoldsmithConfirm')
  div.modal-dialog.modal-sm(role='document')
    div.modal-content
      div.modal-header
        h4.modal-title Give Goldsmith
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') &times;
      div.modal-body
        p Give Goldsmith?
      div.modal-footer
        button.btn.btn-primary#dialogAccept(type='button', data-dismiss='modal') Ok
        button.btn.btn-secondary#dialogCancel(type='button', data-dismiss='modal') Cancel

div.alert.alert-success.alert-dismissible#alertGiveGoldsmithSuccess(role='alert', style="display:none;")
  strong Success 
  span#GiveGoldsmithSuccessMsg Give Goldsmith Successful.
  button.close(type='button', data-hide='alert', aria-label='Close')
    span(aria-hidden='true') &times;

div.alert.alert-danger.alert-dismissible#alertGiveGoldsmithError(role='alert', style="display:none;")
  strong Error 
  span#giveGoldsmithErrorMsg Can not give goldsmith.
  button.close(type='button', data-hide='alert', aria-label='Close')
    span(aria-hidden='true') &times;