div.modal.fade#dialogUndoConfirm(tabindex='-1', role='dialog', aria-labelledby='undoConfirm')
  div.modal-dialog.modal-sm(role='document')
    div.modal-content
      div.modal-header
        h4.modal-title Undo
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') &times;
      div.modal-body
        p Are you sure want to undo?
      div.modal-footer
        button.btn.btn-primary#dialogAccept(type='button', data-dismiss='modal') Ok
        button.btn.btn-secondary#dialogCancel(type='button', data-dismiss='modal') Cancel

div.alert.alert-success.alert-dismissible#alertUndoSuccess(role='alert', style="display:none;")
  strong Success 
  span#UndoSuccessMsg Undo Successful.
  button.close(type='button', data-hide='alert', aria-label='Close')
    span(aria-hidden='true') &times;

div.alert.alert-danger.alert-dismissible#alertUndoError(role='alert', style="display:none;")
  strong Error 
  span#undoErrorMsg Can not undo.
  button.close(type='button', data-hide='alert', aria-label='Close')
    span(aria-hidden='true') &times;